Program Bai1;
Var n : Integer;
    max : Integer;
    x : Integer;
    i : Integer;

(*Procedure FindMax(n: Integer);
    Begin
        For i:= 1 to n Do
        Begin
            if A(.i.) > max then max = A(.i.);
